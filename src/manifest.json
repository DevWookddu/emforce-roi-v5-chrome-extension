{
  "name": "tracking-tracker",
  "description": "Tracking Code Tracker",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "128": "icons/emf.png"
  },
  "browser_action": {
    "default_title": "tracking-tracker",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "webRequest",
    "tabs",
    "<all_urls>"
  ]
}
